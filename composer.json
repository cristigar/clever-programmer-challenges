{
    "name": "cristigar/clever-programmer-challenges",
    "authors": [
        {
            "name": "Cristian Garabagiu",
            "email": "cristian.garabagiu@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleverProgrammerChallenges\\": "src/"
        },
        "files": [
            "src/MyArrayFilter.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleverProgrammerChallenges\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.19",
        "phpunit/phpunit": "^8.4"
    },
    "scripts": {
        "test": [
            "clear",
            "./vendor/bin/phpunit --colors=always"
        ],
        "analyse": [
            "clear",
            "./vendor/bin/phpstan analyse src tests -l max --ansi"
        ]
    }
}
